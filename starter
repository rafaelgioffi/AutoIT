Global $total
Global $pos = 1

Global $resAdobe = "n"
Global $resNet = "n"
Global $resDx = "n"
Global $resChrome = "n"
Global $resJava32 = "n"
Global $resJava64 = "n"
Global $resFlash = "n"
Global $resWrar = "n"
Global $resAdd = "n"
Global $resAtube = "n"
Global $resPdf = "n"

Global $player = "não"
Global $av = "não"
Global $resOffice = "não"

GUISetState(@SW_SHOW)

Func limpa_radios()
    GUICtrlSetState($potplay, $GUI_UNCHECKED)
    GUICtrlSetState($klmpplay, $GUI_UNCHECKED)
    GUICtrlSetState($nplay, $GUI_UNCHECKED)
	GUICtrlSetState($esetav, $GUI_UNCHECKED)
	GUICtrlSetState($nav, $GUI_UNCHECKED)
	GUICtrlSetState($adobeSim, $GUI_UNCHECKED)
	GUICtrlSetState($adobeNao, $GUI_UNCHECKED)
	GUICtrlSetState($netSim, $GUI_UNCHECKED)
	GUICtrlSetState($netNao, $GUI_UNCHECKED)
	GUICtrlSetState($dxSim, $GUI_UNCHECKED)
	GUICtrlSetState($dxNao, $GUI_UNCHECKED)
	GUICtrlSetState($chromeSim, $GUI_UNCHECKED)
	GUICtrlSetState($chromeNao, $GUI_UNCHECKED)
	GUICtrlSetState($java32Sim, $GUI_UNCHECKED)
	GUICtrlSetState($java32Nao, $GUI_UNCHECKED)
	GUICtrlSetState($java64Sim, $GUI_UNCHECKED)
	GUICtrlSetState($java64Nao, $GUI_UNCHECKED)
	GUICtrlSetState($flashSim, $GUI_UNCHECKED)
	GUICtrlSetState($flashNao, $GUI_UNCHECKED)
	GUICtrlSetState($wrarSim, $GUI_UNCHECKED)
	GUICtrlSetState($wrarNao, $GUI_UNCHECKED)
	GUICtrlSetState($addSim, $GUI_UNCHECKED)
	GUICtrlSetState($addNao, $GUI_UNCHECKED)
	GUICtrlSetState($atubeSim, $GUI_UNCHECKED)
	GUICtrlSetState($atubeNao, $GUI_UNCHECKED)
	GUICtrlSetState($pdfSim, $GUI_UNCHECKED)
	GUICtrlSetState($pdfNao, $GUI_UNCHECKED)
	GUICtrlSetState($10_x86, $GUI_UNCHECKED)
	GUICtrlSetState($10_x64, $GUI_UNCHECKED)
	GUICtrlSetState($13_x86, $GUI_UNCHECKED)
	GUICtrlSetState($13_x64, $GUI_UNCHECKED)
	GUICtrlSetState($16_x86, $GUI_UNCHECKED)
	GUICtrlSetState($16_x64, $GUI_UNCHECKED)
 EndFunc

 ;~ funções para marcar radios...
Func marcaplayer(ByRef $item)
   If $item == $potplay Then
	  GUICtrlSetState($potplay, $GUI_CHECKED)
	  GUICtrlSetFont($potplay,10,600)
	  GUICtrlSetFont($klmpplay,10)
	  GUICtrlSetFont($nplay,10)
	  $player = "potplay"
   EndIf

   If $item == $klmpplay Then
	  GUICtrlSetState($klmpplay, $GUI_CHECKED)
	  GUICtrlSetFont($klmpplay,10,600)
	  GUICtrlSetFont($potplay,10)
	  GUICtrlSetFont($nplay,10)
	  $player = "klmp"
   EndIf

   If $item == $nplay Then
	  GUICtrlSetState($nplay, $GUI_CHECKED)
	  GUICtrlSetFont($nplay,10,600)
	  GUICtrlSetFont($potplay,10)
	  GUICtrlSetFont($klmpplay,10)
	  $player = "nplay"
   EndIf
EndFunc

Func marcaav(ByRef $item)
   If $item == $esetav Then
	  GUICtrlSetState($item, $GUI_CHECKED)
	  GUICtrlSetFont($esetav,10, 600)
	  GUICtrlSetFont($nav,10)
	  $av = "eset"
   EndIf

   If $item == $nav Then
	  GUICtrlSetState($item, $GUI_CHECKED)
	  GUICtrlSetFont($nav,10, 600)
	  GUICtrlSetFont($esetav,10)
	  $av = "nav"
   EndIf

EndFunc

Func marcaind(ByRef $sim, ByRef $nao, ByRef $item)
   If $sim == $sim Then
	  GUICtrlSetState($sim, $GUI_CHECKED)
	  GUICtrlSetFont($sim,10, 600)
	  GUICtrlSetFont($nao,10)
	  $item = GUICtrlRead($sim, 1)
   EndIf

EndFunc

Func marcaoffice(ByRef $item)
   If $item == $10_x86 Then
	  GUICtrlSetState($item, $GUI_CHECKED)
	  GUICtrlSetFont($10_x86,10, 600)
	  GUICtrlSetFont($10_x64,10)
	  GUICtrlSetFont($13_x86,10)
	  GUICtrlSetFont($13_x64,10)
	  GUICtrlSetFont($16_x86,10)
	  GUICtrlSetFont($16_x64,10)
	  GUICtrlSetFont($nof,10)
	  $resOffice = "10_x86"
   EndIf
   If $item == $10_x64 Then
	  GUICtrlSetState($item, $GUI_CHECKED)
	  GUICtrlSetFont($10_x64,10, 600)
	  GUICtrlSetFont($10_x86,10)
	  GUICtrlSetFont($13_x86,10)
	  GUICtrlSetFont($13_x64,10)
	  GUICtrlSetFont($16_x86,10)
	  GUICtrlSetFont($16_x64,10)
	  GUICtrlSetFont($nof,10)
	  $resOffice = "10_x64"
   EndIf
   If $item == $13_x86 Then
	  GUICtrlSetState($item, $GUI_CHECKED)
	  GUICtrlSetFont($13_x86,10, 600)
	  GUICtrlSetFont($10_x86,10)
	  GUICtrlSetFont($10_x64,10)
	  GUICtrlSetFont($13_x64,10)
	  GUICtrlSetFont($16_x86,10)
	  GUICtrlSetFont($16_x64,10)
	  GUICtrlSetFont($nof,10)
	  $resOffice = "13_x86"
   EndIf
   If $item == $13_x64 Then
	  GUICtrlSetState($item, $GUI_CHECKED)
	  GUICtrlSetFont($13_x64,10, 600)
	  GUICtrlSetFont($10_x86,10)
	  GUICtrlSetFont($10_x64,10)
	  GUICtrlSetFont($13_x86,10)
	  GUICtrlSetFont($16_x86,10)
	  GUICtrlSetFont($16_x64,10)
	  GUICtrlSetFont($nof,10)
	  $resOffice = "13_x64"
   EndIf
   If $item == $16_x86 Then
	  GUICtrlSetState($item, $GUI_CHECKED)
	  GUICtrlSetFont($16_x86,10, 600)
	  GUICtrlSetFont($10_x86,10)
	  GUICtrlSetFont($10_x64,10)
	  GUICtrlSetFont($13_x86,10)
	  GUICtrlSetFont($13_x64,10)
	  GUICtrlSetFont($16_x64,10)
	  GUICtrlSetFont($nof,10)
	  $resOffice = "16_x86"
   EndIf
   If $item == $16_x64 Then
	  GUICtrlSetState($item, $GUI_CHECKED)
	  GUICtrlSetFont($16_x64,10, 600)
	  GUICtrlSetFont($10_x86,10)
	  GUICtrlSetFont($10_x64,10)
	  GUICtrlSetFont($13_x86,10)
	  GUICtrlSetFont($13_x64,10)
	  GUICtrlSetFont($16_x86,10)
	  GUICtrlSetFont($nof,10)
	  $resOffice = "16_x64"
   EndIf
   If $item == $nof Then
	  GUICtrlSetState($item, $GUI_CHECKED)
	  GUICtrlSetFont($nof,10, 600)
	  GUICtrlSetFont($10_x86,10)
	  GUICtrlSetFont($10_x64,10)
	  GUICtrlSetFont($13_x86,10)
	  GUICtrlSetFont($13_x64,10)
	  GUICtrlSetFont($16_x86,10)
	  GUICtrlSetFont($16_x64,10)
	  $resOffice = "nof"
   EndIf

EndFunc

Func total()
   $total = 0

   If $resAdobe == "Sim" Then
	  $total = $total + 1
   EndIf
   If $resNet == "Sim" Then
	  $total = $total + 1
   EndIf
   If $resDx == "Sim" Then
	  $total = $total + 1
   EndIf
   If $resChrome == "Sim" Then
	  $total = $total + 1
   EndIf
   If $resJava32 == "Sim" Then
	  $total = $total + 1
   EndIf
   If $resJava64 == "Sim" Then
	  $total = $total + 1
   EndIf
   If $resFlash == "Sim" Then
	  $total = $total + 6
   EndIf
	  If $resWrar == "Sim" Then
	  $total = $total + 1
   EndIf
     If $resAdd == "Sim" Then
	  $total = $total + 1
   EndIf
     If $resAtube == "Sim" Then
	  $total = $total + 1
   EndIf
     If $resPdf == "Sim" Then
	  $total = $total + 1
   EndIf

     If $player == "potplay" Then
	  $total = $total + 1
   EndIf
   If $player == "klmp" Then
	  $total = $total + 1
   EndIf

   If $av == "eset" Then
	  $total = $total + 1
   EndIf

   If $resOffice == "10_x86" Then
	  $total = $total + 1
   EndIf
   If $resOffice == "10_x64" Then
	  $total = $total + 1
   EndIf
   If $resOffice == "13_x86" Then
	  $total = $total + 1
   EndIf
   If $resOffice == "13_x64" Then
	  $total = $total + 1
   EndIf
   If $resOffice == "16_x86" Then
	  $total = $total + 1
   EndIf
   If $resOffice == "16_x64" Then
	  $total = $total + 1
   EndIf

EndFunc

;~ limpezas...
Func limpeza()
Sleep(3000)
GUICtrlSetData($status, "Limpando os arquivos temporários, aguarde...")
GUICtrlSetData($barra1, 1)
GUICtrlSetData(1, "1%")
RunWait(@ScriptDir & "\cleartemp.exe")
RunWait(@ScriptDir & "\fechanav.exe")
RunWait("Taskkill /f /im plugin-container*")
RunWait("Taskkill /f /im werfault*")
GUICtrlSetData($barra1, 100)
GUICtrlSetData(100, "1%")
RunWait(@ScriptDir & "\fechanav.exe")
ProcessClose("starter.exe")

EndFunc

While 1
Switch GUIGetMsg()

;~ ao clicar em instalar...
   Case $btn1
;~ validando se tudo está marcado...
;~ avisos...
	    If $player == "não" Then
		 MsgBox(16, "Escolha um Player de vídeo", "Por favor, escolha um player de vídeo/codec para instalar." & @CRLF & "Para não instalar nenhum player/codec, escolha a opção " & Chr(34) & "Sem Player" & Chr(34))
		ContinueLoop
	  EndIf

	  If $av == "não" Then
		 MsgBox(16, "Escolha um Anti-vírus", "Por favor, escolha o anti-vírus para instalar." & @CRLF & "Para não instalar nenhum anti-vírus, escolha a opção " & Chr(34) & "Sem Anti-vírus" & Chr(34))
		 ContinueLoop
	  EndIf

	  If $resOffice = "não" Then
		 MsgBox(16, "Escolha um Office", "Por favor, escolha uma versão do Office para instalar." & @CRLF & "Para não instalar nenhum Office, escolha a opção " & Chr(34) & "Sem Office" & Chr(34))
		 ContinueLoop
	  EndIf

	  If $resAdobe = "n" Then
		 MsgBox(16, "Instalar o Adobe Reader?", "Por favor, escolha se deseja instalar o Adobe Reader ou não." & @CRLF & "Para não instalar o Adobe Reader, escolha a opção " & Chr(34) & "Não" & Chr(34) & " em " & Chr(34) & "Adobe Reader" & Chr(34))
		 ContinueLoop
	  EndIf

	  If $resNet = "n" Then
		 MsgBox(16, "Instalar o .Net Framework 4.6.1?", "Por favor, escolha se deseja instalar o .Net Framework 4.6.1 ou não." & @CRLF & "Para não instalar o .Net Framework 4.6.1, escolha a opção " & Chr(34) & "Não" & Chr(34) & " em " & Chr(34) & ".Net Framework 4.6.1?" & Chr(34))
		 ContinueLoop
	  EndIf

	  If $resDx = "n" Then
		 MsgBox(16, "Instalar o DirectX?", "Por favor, escolha se deseja instalar o DirectX ou não." & @CRLF & "Para não instalar o DirectX, escolha a opção " & Chr(34) & "Não" & Chr(34) & " em " & Chr(34) & "DirectX?" & Chr(34))
		 ContinueLoop
	  EndIf

	  If $resChrome = "n" Then
		 MsgBox(16, "Instalar o Google Chrome?", "Por favor, escolha se deseja instalar o Google Chrome ou não." & @CRLF & "Para não instalar o Google Chrome, escolha a opção " & Chr(34) & "Não" & Chr(34) & " em " & Chr(34) & "Google Chrome?" & Chr(34))
		 ContinueLoop
	  EndIf

	  If $resJava32 = "n" Then
		 MsgBox(16, "Instalar o Java JRE de 32 bits?", "Por favor, escolha se deseja instalar o Java JRE de 32 bits ou não." & @CRLF & "Para não instalar o Java JRE de 32 bits, escolha a opção " & Chr(34) & "Não" & Chr(34) & " em " & Chr(34) & "Java JRE 32 bits?" & Chr(34))
		 ContinueLoop
	  EndIf

	  If $resJava64 = "n" Then
		 MsgBox(16, "Instalar o Java JRE de 64 bits?", "Por favor, escolha se deseja instalar o Java JRE de 64 bits ou não." & @CRLF & "Para não instalar o Java JRE de 64 bits, escolha a opção " & Chr(34) & "Não" & Chr(34) & " em " & Chr(34) & "Java JRE 64 bits?" & Chr(34))
		 ContinueLoop
	  EndIf

