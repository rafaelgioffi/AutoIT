		 MsgBox(16, "Instalar o WinRAR?", "Por favor, escolha se deseja instalar o WinRAR ou não." & @CRLF & "Para não instalar o WinRAR, escolha a opção " & Chr(34) & "Não" & Chr(34) & " em " & Chr(34) & "WinRAR?" & Chr(34))
		 ContinueLoop
	  EndIf

