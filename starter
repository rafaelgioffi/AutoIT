;~ links...
Case $basic
   limpa_radios()
   marcaplayer($nplay)
   marcaav($nav)
   marcaoffice($nof)
   marcaind($adobeSim, $adobeNao, $resAdobe)
   marcaind($netSim, $netNao, $resNet)
   marcaind($dxSim, $dxNao, $resDx)
   marcaind($chromeNao, $chromeSim, $resChrome)
   marcaind($java32Sim, $java32Nao, $resJava32)
   marcaind($java64Nao, $java64Sim, $resJava64)
   marcaind($flashSim, $flashNao, $resFlash)
   marcaind($wrarSim, $wrarNao, $resWrar)
   marcaind($addNao, $addSim, $resAdd)
   marcaind($atubeNao, $atubeSim, $resAtube)
   marcaind($pdfSim, $pdfNao, $resPdf)
   ContinueLoop

;~ basicos...
Case $bas_av
   limpa_radios()
   marcaplayer($nplay)
   marcaav($esetav)
   marcaoffice($nof)
   marcaind($adobeSim, $adobeNao, $resAdobe)
   marcaind($netSim, $netNao, $resNet)
   marcaind($dxSim, $dxNao, $resDx)
   marcaind($chromeNao, $chromeSim, $resChrome)
   marcaind($java32Sim, $java32Nao, $resJava32)
   marcaind($java64Nao, $java64Sim, $resJava64)
   marcaind($flashSim, $flashNao, $resFlash)
   marcaind($wrarSim, $wrarNao, $resWrar)
   marcaind($addNao, $addSim, $resAdd)
   marcaind($atubeNao, $atubeSim, $resAtube)
   marcaind($pdfSim, $pdfNao, $resPdf)
   ContinueLoop

Case $bas_pot
   limpa_radios()
   marcaplayer($potplay)
   marcaav($esetav)
   marcaoffice($nof)
   marcaind($adobeSim, $adobeNao, $resAdobe)
   marcaind($netSim, $netNao, $resNet)
   marcaind($dxSim, $dxNao, $resDx)
   marcaind($chromeNao, $chromeSim, $resChrome)
   marcaind($java32Sim, $java32Nao, $resJava32)
   marcaind($java64Nao, $java64Sim, $resJava64)
   marcaind($flashSim, $flashNao, $resFlash)
   marcaind($wrarSim, $wrarNao, $resWrar)
   marcaind($addNao, $addSim, $resAdd)
   marcaind($atubeNao, $atubeSim, $resAtube)
   marcaind($pdfSim, $pdfNao, $resPdf)
   ContinueLoop

Case $lowprofile
limpa_radios()
   marcaplayer($klmpplay)
   marcaav($esetav)
   marcaoffice($10_x86)
   marcaind($adobeSim, $adobeNao, $resAdobe)
   marcaind($netSim, $netNao, $resNet)
   marcaind($dxSim, $dxNao, $resDx)
