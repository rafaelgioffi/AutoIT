		 MsgBox(16, "Instalar o Auslogic Disk Defrag?", "Por favor, escolha se deseja instalar o Auslogic Disk Defrag ou não." & @CRLF & "Para não instalar o Auslogic Disk Defrag, escolha a opção " & Chr(34) & "Não" & Chr(34) & " em " & Chr(34) & "Auslogic Disk Defrag?" & Chr(34))
		 ContinueLoop
	  EndIf

